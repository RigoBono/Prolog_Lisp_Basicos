(defun doble(X) (* 2 X))


;Funcion para sacar exponencial de un numero
(defun exponencial(X Y) (expt X Y))

;Funcion para obtener raiz cuadrada de un numero
(defun raizC(X) (sqrt X))

;Funcion para obtener un numero aleatorio
(defun randomPro(X) (random X))

;Funcion para incrementar en uno.
(defun incrementa(X) (1+ X))

;Funcion para obtener el valor absoluto
(defun Absoluto(X) (abs X))

;Funcion para obtener el valor maximo de 3 valores
(defun Maximo(X Y Z) (max X Y Z))

;Sacar el coseno de un angulo
(defun Coseno(X) (cos X))

;Sacar la tangente de un angulo
(defun Tangente(X) (tan X))

;Sacar el seno de un angulo
(defun Seno(X) (sin X))

;Devuelve el arcotangente de un angulo
(defun ArcoT(X) (atan X))
